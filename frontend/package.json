{
  "name": "sql-agent-chat",
  "version": "1.0.0",
  "description": "SQL Agent Chat Interface",
  "scripts": {
    "build:css": "tailwindcss -i ./src/input.css -o ./static/css/main.css --minify",
    "watch:css": "tailwindcss -i ./src/input.css -o ./static/css/main.css --watch",
    "install:py": "uv pip install -r requirements.txt",
    "dev": "npm run watch:css"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
}